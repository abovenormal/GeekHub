                    android:textStyle="bold" />
            </LinearLayout>

            <View
                android:id="@+id/receive_line"
                android:visibility="invisible"
                android:layout_width="100dp"
                android:layout_height="3dp"
                android:layout_marginTop="6dp"
                android:background="@color/gray_500" />

        </LinearLayout>


